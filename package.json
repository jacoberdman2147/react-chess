{
  "name": "react-chess",
  "private": true,
  "version": "0.0.1",
  "description": "A simple and lightweight chess implementation in React",
  "type": "module",
  "scripts": {
    "start:frontend": "vite",
    "start:backend": "nodemon server/server.js",
    "start": "concurrently 'npm:start:frontend' 'npm:start:backend'",
    "prod": "env NODE_ENV=production node server/server.js",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacoberdman2147/react-chess.git"
  },
  "keywords": [
    "chess",
    "react"
  ],
  "author": "Jacob Erdman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacoberdman2147/react-chess/issues"
  },
  "homepage": "https://github.com/jacoberdman2147/react-chess#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "ejs": "^3.1.9",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
